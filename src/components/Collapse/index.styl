.collapse-content
  border-left: 1px solid #0D336F;
  border-right: 1px solid #0D336F;
  border-top: 1px solid #0D336F;
  width 450px

  .collapse-header
    font-size: 14px
    color: #CBDEF4
    height 40px
    line-height 40px
    border-bottom  1px solid #0D336F;
    padding-left 30px
    display flex
    justify-content center
    position relative
    margin: 0 -1px -1px 0;

    span:nth-child(1)
      width 85%
    span:nth-child(2)
      width 15%
      color: #00E4FF;

.col-item-content {
  position: relative;
  overflow: auto;
}

.none{
  height: 0
  padding: 0
}